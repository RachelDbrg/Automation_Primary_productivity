@article{kramer2022,
  title = {Managing {{Moose}} from {{Home}}: {{Determining Landscape Carrying Capacity}} for {{Alces}} Alces {{Using Remote Sensing}}},
  shorttitle = {Managing {{Moose}} from {{Home}}},
  author = {Kramer, David W. and Prebyl, Thomas J. and Nibbelink, Nathan P. and Miller, Karl V. and Royo, Alejandro A. and Frair, Jacqueline L.},
  year = {2022},
  month = jan,
  journal = {Forests},
  volume = {13},
