@article{carpenter2009,
  title = {Science for Managing Ecosystem Services: {{Beyond}} the {{Millennium Ecosystem Assessment}}},
  shorttitle = {Science for Managing Ecosystem Services},
  author = {Carpenter, Stephen and Mooney, Harold and Agard, John and Capistrano, Doris and Defries, Ruth and Diaz, Sandra and Dietz, Thomas and Duraiappah, Anantha and {Oteng-Yeboah}, Alfred and Pereira, Henrique and Perrings, Charles and Reid, Walter and Sarukhan, Jos{\'e} and Scholes, Robert and Whyte, Anne},
  year = {2009},
  month = feb,
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {106},
  pages = {1305--12},
  doi = {10.1073/pnas.0808772106},
  abstract = {The Millennium Ecosystem Assessment (MA) introduced a new framework for analyzing social-ecological systems that has had wide influence in the policy and scientific communities. Studies after the MA are taking up new challenges in the basic science needed to assess, project, and manage flows of ecosystem services and effects on human well-being. Yet, our ability to draw general conclusions remains limited by focus on discipline-bound sectors of the full social-ecological system. At the same time, some polices and practices intended to improve ecosystem services and human well-being are based on untested assumptions and sparse information. The people who are affected and those who provide resources are increasingly asking for evidence that interventions improve ecosystem services and human well-being. New research is needed that considers the full ensemble of processes and feedbacks, for a range of biophysical and social systems, to better understand and manage the dynamics of the relationship between humans and the ecosystems on which they rely. Such research will expand the capacity to address fundamental questions about complex social-ecological systems while evaluating assumptions of policies and practices intended to advance human well-being through improved ecosystem services.},
  keywords = {ALIRE},
  file = {..\\..\\Zotero\\storage\\ZI9VUES4\\carpenter2009.pdf.pdf;Carpenter et al_2009_Science for managing ecosystem services.pdf}
}

@article{carroll2019,
  title = {A Review of Methods for Quantifying Spatial Predator{\textendash}Prey Overlap},
  author = {Carroll, Gemma and Holsman, Kirstin K. and Brodie, Stephanie and Thorson, James T. and Hazen, Elliott L. and Bograd, Steven J. and Haltuch, Melissa A. and Kotwicki, Stan and Samhouri, Jameal and Spencer, Paul and {Willis-Norton}, Ellen and Selden, Rebecca L.},
  year = {2019},
  journal = {Global Ecology and Biogeography},
  volume = {28},
  number = {11},
  pages = {1561--1577},
  issn = {1466-8238},
  doi = {10.1111/geb.12984},
  urldate = {2023-03-21},
  abstract = {Background Studies that attempt to measure shifts in species distributions often consider a single species in isolation. However, understanding changes in spatial overlap between predators and their prey might provide deeper insight into how species redistribution affects food web dynamics. Predator{\textendash}prey overlap metrics Here, we review a suite of 10 metrics [range overlap, area overlap, the local index of collocation (Pianka's O), Hurlbert's index, biomass-weighted overlap, asymmetrical alpha, Schoener's D, Bhattacharyya's coefficient, the global index of collocation and the AB ratio] that describe how two species overlap in space, using concepts such as binary co-occurrence, encounter rates, spatial niche similarity, spatial independence, geographical similarity and trophic transfer. We describe the specific ecological insights that can be gained using each overlap metric, in order to determine which is most appropriate for describing spatial predator{\textendash}prey interactions for different applications. Simulation and case study We use simulated predator and prey distributions to demonstrate how the 10 metrics respond to variation in three types of predator{\textendash}prey interactions: changing spatial overlap between predator and prey, changing predator population size and changing patterns of predator aggregation in response to prey density. We also apply these overlap metrics to a case study of a predatory fish (arrowtooth flounder, Atheresthes stomias) and its prey (juvenile walleye pollock, Gadus chalcogrammus) in the Eastern Bering Sea, AK, USA. We show how the metrics can be applied to understand spatial and temporal variation in the overlap of species distributions in this rapidly changing Arctic ecosystem. Conclusions Using both simulated and empirical data, we provide a roadmap for ecologists and other practitioners to select overlap metrics to describe particular aspects of spatial predator{\textendash}prey interactions. We outline a range of research and management applications for which each metric may be suited.},
  langid = {english},
  keywords = {arrowtooth flounder,climate change,cold pool,Eastern Bering Sea,ecosystem models,predator{\textendash}prey overlap,spatial overlap,species distribution models,species interactions,walleye pollock},
  file = {..\\..\\Zotero\\storage\\MCFYV5MQ\\Carroll-et-al.-2019-GEB.pdf;..\\..\\Zotero\\storage\\RW4W84BM\\Carroll-et-al.-2019-GEB.pdf;Carroll et al_2019_A review of methods for quantifying spatial predator–prey overlap.pdf}
}

