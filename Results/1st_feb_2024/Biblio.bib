@article{crete2001,
  title = {Comparative {{Effects}} on {{Plants}} of {{Caribou}}/{{Reindeer}}, {{Moose}} and {{White-tailed Deer Herbivory}}},
  author = {Cr{\^e}te, Michel and Ouellet, Jean-Pierre and Lesage, Louis},
