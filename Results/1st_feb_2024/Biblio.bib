@article{bergerud1986,
  title = {Dynamics of Caribou and Wolves in Northern {{British Columbia}}},
  author = {Bergerud, A. T. and Elliot, J. P.},
  year = {1986},
  month = jul,
  journal = {Canadian Journal of Zoology},
  volume = {64},
  number = {7},
  pages = {1515--1529},
