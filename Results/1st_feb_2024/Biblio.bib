  shorttitle = {Diffusion{\textendash}Reaction Compartmental Models Formulated in a Continuum Mechanics Framework},
  author = {Viguerie, Alex and Veneziani, Alessandro and Lorenzo, Guillermo and Baroli, Davide and {Aretz-Nellesen}, Nicole and Patton, Alessia and Yankeelov, Thomas E. and Reali, Alessandro and Hughes, Thomas J. R. and Auricchio, Ferdinando},
