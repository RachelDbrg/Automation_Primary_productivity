@article{tardy2021,
  title = {Mechanistic Movement Models Reveal Ecological Drivers of Tick-Borne Pathogen Spread},
  author = {Tardy, Olivia and Bouchard, Catherine and Chamberland, Eric and Fortin, Andr{\'e} and Lamirande, Patricia and Ogden, Nicholas H. and Leighton, Patrick A.},
  year = {2021},
