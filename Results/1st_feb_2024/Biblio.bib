@article{bachand2014,
  title = {Functional {{Responses}} and {{Resilience}} of {{Boreal Forest Ecosystem}} after {{Reduction}} of {{Deer Density}}},
  author = {Bachand, Marianne and Pellerin, St{\'e}phanie and Moretti, Marco and Aubin, Isabelle and Tremblay, Jean-Pierre and C{\^o}t{\'e}, Steeve D. and Poulin, Monique},
  year = {2014},
