@article{hashemi2022,
  title = {The {{Review}} of {{Ecological Network Indicators}} in {{Graph Theory Context}}: 2014{\textendash}2021},
  shorttitle = {The {{Review}} of {{Ecological Network Indicators}} in {{Graph Theory Context}}},
  author = {Hashemi, Rastegar and Darabi, Hassan},
  year = {2022},
  month = apr,
