@article{rattanakul2019,
  title = {Analysis of Advection-Diffusion-Reaction Model for Fish Population Movement with Impulsive Tagging: Stability and Traveling Wave Solution},
  shorttitle = {Analysis of Advection-Diffusion-Reaction Model for Fish Population Movement with Impulsive Tagging},
  author = {Rattanakul, Chontita and Lenbury, Yongwimon and Suksamran, Jeerawan},
