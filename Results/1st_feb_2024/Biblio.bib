@article{fortin2015a,
  title = {A Spatial Theory for Characterizing Predator{\textendash}Multiprey Interactions in Heterogeneous Landscapes},
  author = {Fortin, Daniel and Buono, Pietro-Luciano and Schmitz, Oswald J. and Courbin, Nicolas and Losier, Chrystel and {St-Laurent}, Martin-Hugues and Drapeau, Pierre and Heppell, Sandra and Dussault, Claude and Brodeur, Vincent and Mainguy, Julien},
  year = {2015},
