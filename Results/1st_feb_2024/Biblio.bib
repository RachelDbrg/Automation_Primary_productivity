@article{al-moqbali2018,
  title = {Prey{\textendash}{{Predator Models}} with {{Variable Carrying Capacity}}},
  author = {{Al-Moqbali}, Mariam and {Al-Salti}, Nasser and Elmojtaba, Ibrahim},
  year = {2018},
  month = jun,
  journal = {Mathematics},
  volume = {6},
  number = {6},
  pages = {102},
  issn = {2227-7390},
  doi = {10.3390/math6060102},
  urldate = {2022-08-16},
  abstract = {Prey{\textendash}predator models with variable carrying capacity are proposed. These models are more realistic in modeling population dynamics in an environment that undergoes changes. In particular, prey{\textendash}predator models with Holling type I and type II functional responses, incorporating the idea of a variable carrying capacity, are considered. The carrying capacity is modeled by a logistic equation that increases sigmoidally between an initial value {$\kappa$}0 {$>$} {$\kappa$}1 (a lower bound for the carrying capacity) and a final value {$\kappa$}1 + {$\kappa$}2 (an upper bound for the carrying capacity). In order to examine the effect of the variable carrying capacity on the prey{\textendash}predator dynamics, the two models were analyzed qualitatively using stability analysis and numerical solutions for the prey, and the predator population densities were obtained. Results on global stability and Hopf bifurcation of certain equilibrium points have been also presented. Additionally, the effect of other model parameters on the prey{\textendash}predator dynamics has been examined. In particular, results on the effect of the handling parameter and the predator's death rate, which has been taken to be the bifurcation parameter, are presented.},
  langid = {english},
  keywords = {ALIRE},
  file = {..\\..\\Zotero\\storage\\LCAYIZLU\\mathematics-06-00102.pdf;..\\..\\Zotero\\storage\\RXPGHAVQ\\al-moqbali2018.pdf.pdf;Al-Moqbali et al_2018_Prey–Predator Models with Variable Carrying Capacity.pdf}
}

@article{albert2022,
  title = {The Hidden Role of Multi-Trophic Interactions in Driving Diversity{\textendash}Productivity Relationships},
  author = {Albert, Georg and Gauzens, Benoit and Loreau, Michel and Wang, Shaopeng and Brose, Ulrich},
  year = {2022},
  journal = {Ecology Letters},
  volume = {25},
  number = {2},
  pages = {405--415},
  issn = {1461-0248},
  doi = {10.1111/ele.13935},
  urldate = {2023-08-18},
  abstract = {Resource-use complementarity of producer species is often invoked to explain the generally positive diversity{\textendash}productivity relationships. Additionally, multi-trophic interactions that link processes across trophic levels have received increasing attention as a possible key driver. Given that both are integral to natural ecosystems, their interactive effect should be evident but has remained hidden. We address this issue by analysing diversity{\textendash}productivity relationships in a simulation experiment of producer communities nested within complex food-webs, manipulating resource-use complementarity and multi-trophic animal richness. We show that these two mechanisms interactively create diverse communities of complementary producer species. This shapes diversity{\textendash}productivity relationships such that their joint contribution generally exceeds their individual effects. Specifically, multi-trophic interactions in animal-rich ecosystems facilitate producer coexistence by preventing competitive exclusion despite overlaps in resource-use, which increases the realised complementarity. The interdependence of food-webs and producer complementarity in creating biodiversity{\textendash}productivity relationships highlights the importance to adopt a multi-trophic perspective on biodiversity{\textendash}ecosystem functioning relationships.},
  copyright = {{\textcopyright} 2021 The Authors. Ecology Letters published by John Wiley \& Sons Ltd.},
  langid = {english},
  keywords = {biodiversity-ecosystem functioning,complex food-webs,Partial reading,primary production,resource-use complementarity,selection,trophic interaction,vertical diversity},
  file = {Albert et al_2022_The hidden role of multi-trophic interactions in driving diversity–productivity.pdf}
}

