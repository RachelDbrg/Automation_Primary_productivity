@article{turbek2016,
  title = {Scientific {{Writing Made Easy}}: {{A Step-by-Step Guide}} to {{Undergraduate Writing}} in the {{Biological Sciences}}},
  shorttitle = {Scientific {{Writing Made Easy}}},
  author = {Turbek, Sheela P. and Chock, Taylor M. and Donahue, Kyle and Havrilla, Caroline A. and Oliverio, Angela M. and Polutchko, Stephanie K. and Shoemaker, Lauren G. and Vimercati, Lara},
