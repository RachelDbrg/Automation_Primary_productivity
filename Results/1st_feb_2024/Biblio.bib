@article{fibbi2019,
  title = {Impacts of Climate Change on the Gross Primary Production of {{Italian}} Forests},
  author = {Fibbi, Luca and Moriondo, Marco and Chiesi, Marta and Bindi, Marco and Maselli, Fabio},
  year = {2019},
  month = jun,
  journal = {Annals of Forest Science},
