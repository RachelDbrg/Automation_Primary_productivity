@article{fuller1980,
  title = {Wolf {{Population Dynamics}} and {{Prey Relationships}} in {{Northeastern Alberta}}},
  author = {Fuller, Todd K. and Keith, Lloyd B.},
  year = {1980},
  journal = {The Journal of Wildlife Management},
  volume = {44},
