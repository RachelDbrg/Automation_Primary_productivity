@article{eros2019,
  title = {The {{Landscape Ecology}} of {{Rivers}}: From {{Patch-Based}} to {{Spatial Network Analyses}}},
  shorttitle = {The {{Landscape Ecology}} of {{Rivers}}},
  author = {Er{\H o}s, Tibor and Lowe, Winsor H.},
  year = {2019},
