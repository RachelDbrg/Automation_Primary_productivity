@article{hervieux2014,
  title = {Managing Wolves ( {{{\emph{Canis}}}} {\emph{Lupus}} ) to Recover Threatened Woodland Caribou ( {{{\emph{Rangifer}}}} {\emph{Tarandus Caribou}} ) in {{Alberta}}},
  author = {Hervieux, Dave and Hebblewhite, Mark and Stepnisky, Dave and Bacon, Michelle and Boutin, Stan},
