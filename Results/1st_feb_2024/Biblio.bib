@article{andersson1977,
  title = {Influence of {{Predation}} on {{Rodent Populations}}},
  author = {Andersson, Malte and Erlinge, S.},
  year = {1977},
  month = jan,
  journal = {Oikos},
  volume = {29},
  pages = {591},
  doi = {10.2307/3543597},
  abstract = {Predation on populations of small rodents is reviewed. Various types of predators are distinguished; specialists, generalists, residents, and "nomadic" species. Their functional and numerical responses to changes in prey density, and their influence on small rodent numbers, are discussed. The shape of the functional response curve of the predators may be important for the dynamics of the predator-prey system. Generalists with access to alternative prey tend to stabilize rodent numbers, as do migrating specialists, whereas resident specialists may be destabilizing. Field studies on cyclic rodent populations point to the importance of predation during and after the decline phase, and indicate that predation may increase the amplitude and prolong the period of the cycles. Other studies indicate a strong influence of predation on rodent numbers in areas with a diversified prey and predator fauna. /// {\cyrchar\CYRO}{\cyrchar\cyrp}{\cyrchar\cyri}{\cyrchar\cyrs}{\cyrchar\cyra}{\cyrchar\cyrn}{\cyrchar\cyro} {\cyrchar\cyrh}{\cyrchar\cyri}{\cyrchar\cyrshch}{\cyrchar\cyrn}{\cyrchar\cyri}{\cyrchar\cyrch}{\cyrchar\cyre}{\cyrchar\cyrs}{\cyrchar\cyrt}{\cyrchar\cyrv}{\cyrchar\cyro} {\cyrchar\cyrn}{\cyrchar\cyra} {\cyrchar\cyrp}{\cyrchar\cyro}{\cyrchar\cyrp}{\cyrchar\cyru}{\cyrchar\cyrl}{\cyrchar\cyrya}{\cyrchar\cyrc}{\cyrchar\cyri}{\cyrchar\cyrya}{\cyrchar\cyrh} {\cyrchar\cyrm}{\cyrchar\cyre}{\cyrchar\cyrl}{\cyrchar\cyrk}{\cyrchar\cyri}{\cyrchar\cyrh} {\cyrchar\cyrg}{\cyrchar\cyrr}{\cyrchar\cyrery}{\cyrchar\cyrz}{\cyrchar\cyru}{\cyrchar\cyrn}{\cyrchar\cyro}{\cyrchar\cyrv}. {\cyrchar\CYRR}{\cyrchar\cyra}{\cyrchar\cyrz}{\cyrchar\cyrl}{\cyrchar\cyri}{\cyrchar\cyrch}{\cyrchar\cyra}{\cyrchar\cyryu}{\cyrchar\cyrt}{\cyrchar\cyrs}{\cyrchar\cyrya} {\cyrchar\cyrr}{\cyrchar\cyra}{\cyrchar\cyrz}{\cyrchar\cyrn}{\cyrchar\cyrery}{\cyrchar\cyre} {\cyrchar\cyrt}{\cyrchar\cyri}{\cyrchar\cyrp}{\cyrchar\cyrery} {\cyrchar\cyrh}{\cyrchar\cyri}{\cyrchar\cyrshch}{\cyrchar\cyrn}{\cyrchar\cyr